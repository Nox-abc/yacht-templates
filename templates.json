        "name": "VERSION",
        "label": "VERSION",
        "default": "LATEST"
      },
      {
        "name": "MEMORY",
        "label": "MEMORY",
        "default": "2G"
      },
      {
        "name": "TYPE",
        "label": "TYPE",
        "default": "VANILLA"
      }
    ]
  },
