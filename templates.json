[
  {
    "type": 1,
    "title": "Python Bot (Discord/Custom)",
    "name": "python-bot",
    "description": "Container Python pour bots Discord, scripts automatisés, ou applications Python",
    "logo": "https://www.python.org/static/community_logos/python-logo.png",
    "image": "python:3.11-slim",
    "categories": ["Bots", "Development"],
    "platform": "linux",
    "restart_policy": "unless-stopped",
    "command": "sh -c \"pip install discord.py requests && python /app/main.py\"",
    "volumes": [
      {
        "container": "/app",
        "bind": "!apps/python-bot"
      }
    ],
    "env": [
      {
        "name": "DISCORD_TOKEN",
        "label": "DISCORD_TOKEN",
        "default": ""
      }
    ]
  },
  {
    "type": 1,
    "title": "Node.js Bot/App",
    "name": "nodejs-app",
    "description": "Container Node.js pour bots Discord, serveurs web, ou applications JavaScript",
    "logo": "https://nodejs.org/static/images/logo.svg",
    "image": "node:18-slim",
    "categories": ["Bots", "Development", "Web"],
    "platform": "linux",
    "restart_policy": "unless-stopped",
    "command": "sh -c \"npm install && node /app/index.js\"",
    "ports": [
      {
        "App": "3000:3000/tcp"
      }
    ],
    "volumes": [
      {
        "container": "/app",
        "bind": "!apps/nodejs-app"
      }
    ]
  },
